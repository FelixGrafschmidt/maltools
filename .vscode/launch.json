	"version": "0.2.0",
	"configurations": [
		{
			"type": "java",
			"name": "Debug (Launch) - Current File",
			"request": "launch",
			"mainClass": "${file}"
		},
		{
			"type": "java",
			"name": "SpringBoot",
			"request": "launch",
			"console": "externalTerminal",
			"mainClass": "moe.maltools.comparator.ComparatorApplication",
			"projectName": "maltools"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Vue",
			"url": "http://localhost:8082",
			"webRoot": "${workspaceFolder}/frontend"
		}
	],
	"compounds": [
		{
			"name": "Server/Client",
			"configurations": [
				"SpringBoot",
				"Vue"
			]
		}
	]
}
